    "globals": [
      "cartodb",
      "cdb",
      "jasmine",
      "describe",
      "xdescribe",
      "beforeEach",
      "afterEach",
      "spyOn",
      "it",
      "xit",
      "expect",
      "pending"
    ],
    "ignore": [
      "/*.js",
      "/dist",
      "/doc",
      "/examples",
      "/grunt",
      "/styleguide",
      "/themes",
      "/vendor"
    ]
